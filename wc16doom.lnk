
# DOOM16WC.EXE Linker directive file

#option quiet
option map
#option stack=65536
#option stub=wstub
option static
option verbose
libpath %WATCOM%\lib286
libpath %WATCOM%\lib286\dos
lib noemu87.lib
format dos
name DOOM16WC
